#User function Template for python3

'''
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
        self.prev = None
'''

class Solution():
#Function to sort the given doubly linked list using Merge Sort.
    def sortDoubly(self,head):
    #Your code here
        li=[]
        temp=head
        while temp:
            li.append(temp.data)
            temp=temp.next
        li.sort()
        l3=DoublyLinkedList()
        for i in li:
            l3.append(i)
        return l3.head


